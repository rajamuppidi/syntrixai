# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id

# S3 Buckets
S3_CLINICAL_NOTES_BUCKET=pa-agent-clinical-notes-${AWS_ACCOUNT_ID}
S3_EVIDENCE_BUCKET=pa-agent-evidence-docs-${AWS_ACCOUNT_ID}

# DynamoDB
DYNAMODB_TABLE=pa-cases

# Bedrock
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_REGION=us-east-1
BEDROCK_AGENT_ID=your-agent-id-here
